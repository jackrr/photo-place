<Alloy>
	<Window id="largeImage" class="container">
		<View id="titleBar">
			<Label id="title"/>
			<Label id="titleDate"/>
		</View>
		<View id="imageMapContainer">
			<ImageView id="image"/>
		</View>
		<Label id="caption"/>
		<View id="threadObject"/>
		<View id="actionBar">
			<Label id="newThread" onClick="newThread"/>
			<Label id="back" onClick="back"/>
		</View>
	</Window>
</Alloy>