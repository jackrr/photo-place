<Alloy>
	<Window class="container">
		<View id="uploadBar">
			<Label id="placeName"/>
			<Button id="uploadPhoto" onClick="choosePhoto"/>
		</View>
		<View id="navigation">
			<View class="buttonContainer" id="globalContainer" onClick="globeButt">
				<Label id="global" class="navButt"/>
			</View>
			<View class="buttonContainer" id="nearbyContainer" onClick="nearButt">
				<Label id="nearby" class="navButt"/>
			</View>
			<View class="buttonContainer" id="myPlaceContainer" onClick="myPlaceButt">
				<Label id="myPlace" class="navButt"/>
			</View>
			<View class="buttonContainer" id="myPhotosContainer" onClick="myPhotosButt">
				<Label id="myPhotos" class="navButt"/>
			</View>
		</View>
		<TableView id="tableView"/>
	</Window>
</Alloy>