<Alloy>
	<Window class="container">
		<View id="uploadBar">
			<Label id="back" onClick="eliminate">Back</Label>
			<Button id="uploadPhoto" onClick="choosePhoto"></Button>
		</View>
		<View id="navigation">
			<View class="buttonContainer selected" id="globalContainer" onClick="globeButt">
				<Label id="global" class="navButt"></Label>
			</View>
			<View class="buttonContainer" id="nearbyContainer" onClick="nearButt">
				<Label id="nearby" class="navButt">Nearby</Label>
			</View>
			<View class="buttonContainer" id="myPlaceContainer" onClick="myPlaceButt">
				<Label id="myPlace" class="navButt"></Label>
			</View>
			<View class="buttonContainer" id="myPhotosContainer" onClick="myPhotosButt">
				<Label id="myPhotos" class="navButt">My Photos</Label>
			</View>
		</View>
		<TableView id="tableView"></TableView>
	</Window>
</Alloy>