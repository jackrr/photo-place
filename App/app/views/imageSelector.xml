<Alloy>
	<Window id="imageSelector" class="container">
		<View id="imageArea">
			<View id="overlayRegion"></View>
			<ImageView id="image"></ImageView>
		</View>
		<View id="textInteracts">
			<View id="nameArea" class='interactRow'>
				<!--<Label id="nameLabel"></Label>-->
				<TextField id="name"></TextField>
			</View>
			<View id="doneArea" class='interactRow'>
				<Label id="cancel" onClick="cancel"></Label>
				<Label id="done" onClick="done"></Label>
			</View>
		</View>
	</Window>
</Alloy>